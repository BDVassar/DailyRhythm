import { beatService } from "../services/BeatService.js"
import BaseController from "../utils/BaseController.js"

export class BeatController extends BaseController {
constructor(){
    super('api/beats')

}
    async getAllBeats(req, res, next) {
    try {
    const beats = await beatService
     return res.send()
    } catch (error) {
     next('error getting all beats in beat controller', error)
    }
    }

}